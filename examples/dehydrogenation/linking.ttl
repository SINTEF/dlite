@prefix : <http://onto-ns.com/ontology/dehydrogenation/linking#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix emmo: <http://emmo.info/emmo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dlite: <http://emmo.info/datamodel/dlitemodel#> .
@base <http://onto-ns.com/ontology/dehydrogenation/linking> .

<http://onto-ns.com/ontology/dehydrogenation/linking> rdf:type owl:Ontology ;
    owl:versionIRI <http://onto-ns.com/ontology/dehydrogenation/0.1/linking> .

:mapsTo rdf:type rdf:Property ;
    rdfs:domain rdfs:Resource ;
    rdfs:range rdfs:Class .

dlite:Metadata rdf:type owl:Class .
dlite:Property rdf:type owl:Class .
emmo:Energy rdf:type owl:Class .
emmo:Model rdf:type owl:Class .

:GroundStateEnergy rdf:type owl:Class ;
    rdfs:subClassOf emmo:Energy .
:MoleculeModel rdf:type owl:Class ;
    rdfs:subClassOf emmo:Model .

<http://onto-ns.com/meta/0.1/Molecule> rdf:type
      owl:NamedIndividual ,
      dlite:Metadata ;
    :mapsTo :MoleculeModel .

<http://onto-ns.com/meta/0.1/Molecule#energy> rdf:type
      owl:NamedIndividual ,
      dlite:Property ;
    :mapsTo :GroundStateEnergy .
