{
  "uri": "http://onto-ns.com/meta/0.1/PrecipitateStatistics",
  "description": "Metadata for precipitate statistics.",
  "dimensions": {
    "nconditions": "Number of conditions."
  },
  "properties": {
    "alloy": {
      "type": "string",
      "shape": ["nconditions"],
      "description": "Alloy destination."
    },
    "condition": {
      "type": "string",
      "shape": ["nconditions"],
      "description": "Condition identifier."
    },
    "precipitate": {
      "type": "string",
      "shape": ["nconditions"],
      "description": "Precipitate identifier."
    },
    "number_density": {
      "type": "float64",
      "shape": ["nconditions"],
      "unit": "#/um3",
      "description": "Precipitate number density."
    },
    "avg_length": {
      "type": "float64",
      "shape": ["nconditions"],
      "unit": "nm",
      "description": "Average precipitate length."
    },
    "avg_crossection": {
      "type": "float64",
      "shape": ["nconditions"],
      "unit": "nm2",
      "description": "Average precipitate cross-section."
