   match-leak-kinds: reachable
   fun:calloc
   fun:H5TS_cancel_count_inc
   fun:H5open
   fun:dh5_open
   fun:dlite_storage_open
}

{
   H5Fcreate-still_reachable
   Memcheck:Leak
